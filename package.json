{
   "name": "painting-journal",
   "version": "0.0.1-SNAPSHOT",
   "description": "Create and save paint palettes for your miniatures",
   "author": "Bernardo Mart√≠nez Garrido",
   "homepage": "https://github.com/bernardo-mg/painting-journal",
   "license": "MIT",
   "keywords": [
      "miniature",
      "palette",
      "painting"
